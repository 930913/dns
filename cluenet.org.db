; vim: ft=bindzone:ts=8:sw=8:nowrap:noet:
$ORIGIN cluenet.org.
$TTL 2h

@	SOA	ns1 hostmaster (
			1350926291	; serial
			1h		; refresh
			900		; retry
			1209600		; expire
			300		; minttl
		)

	; TODO: need to update glue records in dynadot
	; TODO: out-of-domain nameservers
